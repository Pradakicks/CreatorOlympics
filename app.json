{
  "expo": {
    "name": "Creator Olympics",
    "slug": "creator-olympics",
    "scheme": "creatorolympics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/la28-olympics.jpg",
    "splash": {
      "image": "./assets/la28-olympics.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.creatorolympics"
    },
    "android": {
      "adaptiveIcon": {},
      "package": "com.anonymous.creatorolympics"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/la28-olympics.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset"
    ]
  }
}
